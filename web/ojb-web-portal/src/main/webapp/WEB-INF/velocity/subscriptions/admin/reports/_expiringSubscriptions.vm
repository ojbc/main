 <script type="text/javascript">
 
  $(function() {
  	$('#searchBarButtonDiv').hide();    		
  	
    var oTable = $("#searchResultsTable").DataTable({
		 "pagingType": "full_numbers",
		 "pageLength": 25,
		 stateSave: true,
		 responsive: true, 
		 colReorder: true
	});
	
	new $.fn.dataTable.FixedHeader( oTable );
	
	$("#subscriptionButtons").appendTo("#searchResultsTable_length");	  
	
	$('#returnToSearchForm').click (function() { 
		return adminPage.populateSubsAdminContentContentFromUrl(context + 'subscriptions/admin/expiringSubscriptionsForm')")
    });
			
    
  });
    
</script>
	
<div style="padding:10px">
	<span id="subscriptionButtons">
		<a id="exportToExcel" href=context + 'subscriptions/admin/exportExpiringSubscriptions')" class="blueButton viewDetails"><img style="margin-right:6px;" src="../static/images/Search%20Detail/icon-close.png"/>EXPORT TO EXCEL</a>
		<a id="returnToSearchForm" href="#" class="blueButton viewDetails"><img style="margin-right:6px;" src="../static/images/Search%20Detail/icon-close.png"/>RETURN</a>
	</span>    				
	<h5>Expiring Subscriptions</h5>
	<table class="searchResultsTable table table-striped table-bordered mt-3" id="searchResultsTable">
		<thead>
			<tr>
				<th>AGENCY</th>
				<th>ORI</th>
				<th>SUBSCRIPTION OWNER</th>
				<th>SUBJECT</th>
				<th>CASE NUMBER</th>
				<th>START DATE</th>
				<th>LAST VALIDATED</th>
				<th>END DATE</th>
				<th>VALIDATION DUE</th>
			</tr>
		</thead>
		<tbody>
	        #foreach($subscription in $expiringSubscriptions)
	        <tr>
	        	<td>$subscription.agencyName</td>
	        	<td>$subscription.ori</td>
	        	<td>$subscription.subscriptionOwnerName</td>
	        	<td>$subscription.personFullName</td>
	        	<td>$subscription.agencyCaseNumber</td>
	        	<td>$subscription.startDateString</td>
	        	<td>$subscription.lastValidationDateString</td>
	        	<td>$subscription.endDateString</td>
	        	<td>$subscription.validationDueDateString</td>
        	</tr>
	        #end
		</tbody>
	</table> 								
</div>
