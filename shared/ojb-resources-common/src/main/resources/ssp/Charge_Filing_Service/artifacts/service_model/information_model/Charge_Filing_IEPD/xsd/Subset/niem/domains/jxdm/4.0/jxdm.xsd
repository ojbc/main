      <xsd:documentation>Details about a legal capacity in which a judicial official is able to practice law.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="Offense" type="nc:IncidentType" nillable="true">
    <xsd:annotation>
