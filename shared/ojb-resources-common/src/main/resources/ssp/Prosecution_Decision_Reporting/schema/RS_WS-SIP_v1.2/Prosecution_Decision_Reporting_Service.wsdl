<wsdl:definitions xmlns="http://schemas.xmlsoap.org/wsdl/"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
   xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy"
   xmlns:wsaw="http://www.w3.org/2006/05/addressing/wsdl"
   xmlns:wsu="http://docs.oasisopen.org/wss/2004/01/oasis200401wsswssecurityutility1.0.xsd"
   xmlns:pddoc="http://ojbc.org/IEPD/Exchange/ProsecutionDecisionReport/1.0"
   xmlns:pdudoc="http://ojbc.org/IEPD/Exchange/ProsecutionDecisionReportUpdate/1.0"
   xmlns:tns="http://ojbc.org/Services/WSDL/ProsecutionDecisionReportingService/1.0"
   targetNamespace="http://ojbc.org/Services/WSDL/ProsecutionDecisionReportingService/1.0">
   <wsdl:import
       namespace="http://ojbc.org/PolicyAssertions/WSDL/GFIPM_SecurityPolicy/1.0/"
       location="../../../../policyassertions/GFIPMSystemToSystem.wsdl" />
   <wsdl:types>
       <xsd:schema>
           <xsd:import
               namespace="http://ojbc.org/IEPD/Exchange/ProsecutionDecisionReport/1.0"
               schemaLocation="../../artifacts/service_model/information_model/IEPD/xsd/ProsecutionDecisionReport.xsd" />
           <xsd:import
               namespace="http://ojbc.org/IEPD/Exchange/ProsecutionDecisionReportUpdate/1.0"
               schemaLocation="../../artifacts/service_model/information_model/IEPD/xsd/ProsecutionDecisionReportUpdate.xsd" />
       </xsd:schema>
   </wsdl:types>
   <wsdl:message name="ProsecutionDecisionReport">
       <wsdl:part name="ProsecutionDecisionReport" element="pddoc:ProsecutionDecisionReport" />
   </wsdl:message>
   <wsdl:message name="ProsecutionDecisionReportUpdate">
       <wsdl:part name="ProsecutionDecisionReportUpdate" element="pdudoc:ProsecutionDecisionReportUpdate" />
   </wsdl:message>
   <wsdl:portType name="ProsecutionDecisionReportingService">
       <wsdl:operation name="ReportProsecutionDecision">
           <wsdl:input name="ProsecutionDecisionReport" message="tns:ProsecutionDecisionReport" />
       </wsdl:operation>
       <wsdl:operation name="ReportProsecutionDecisionUpdate">
           <wsdl:input name="ProsecutionDecisionReportUpdate" message="tns:ProsecutionDecisionReportUpdate" />
       </wsdl:operation>
   </wsdl:portType>
   <!--  Secure, GFIPMconforming binding -->
   <wsdl:binding
       name="ProsecutionDecisionReportingServiceSecureServiceSOAPBinding"
       type="tns:ProsecutionDecisionReportingService">
       <soap:binding style="document"
           transport="http://schemas.xmlsoap.org/soap/http" />
       <wsp:PolicyReference URI="#GFIPMSystemToSystem" />
       <wsdl:operation name="ReportProsecutionDecision">
           <soap:operation
               soapAction="http://ojbc.org/Services/WSDL/ProsecutionDecisionReportingService/1.0/ReportProsecutionDecision"
               style="document" />
           <wsdl:input>
               <soap:body use="literal" />
           </wsdl:input>
       </wsdl:operation>
       <wsdl:operation name="ReportProsecutionDecisionUpdate">
           <soap:operation
               soapAction="http://ojbc.org/Services/WSDL/ProsecutionDecisionReportingService/1.0/ReportProsecutionDecisionUpdate"
               style="document" />
           <wsdl:input>
               <soap:body use="literal" />
           </wsdl:input>
       </wsdl:operation>
   </wsdl:binding>
   <!-- Unsecure binding -->
   <wsdl:binding
       name="ProsecutionDecisionReportingServiceSOAPBinding"
       type="tns:ProsecutionDecisionReportingService">
       <soap:binding style="document"
           transport="http://schemas.xmlsoap.org/soap/http" />
       <wsdl:operation name="ReportProsecutionDecision">
           <soap:operation
               soapAction="http://ojbc.org/Services/WSDL/ProsecutionDecisionReportingService/1.0/ReportProsecutionDecision"
               style="document" />
           <wsdl:input>
               <soap:body use="literal" />
           </wsdl:input>
       </wsdl:operation>
       <wsdl:operation name="ReportProsecutionDecisionUpdate">
           <soap:operation
               soapAction="http://ojbc.org/Services/WSDL/ProsecutionDecisionReportingService/1.0/ReportProsecutionDecisionUpdate"
               style="document" />
           <wsdl:input>
               <soap:body use="literal" />
           </wsdl:input>
       </wsdl:operation>
   </wsdl:binding> 
   <wsdl:service name="ProsecutionDecisionReportingServiceService">
       <wsdl:port name="ProsecutionDecisionReportingService"
           binding="tns:ProsecutionDecisionReportingServiceSOAPBinding">
           <soap:address location="http://localhost" />
       </wsdl:port>
       <wsdl:port name="ProsecutionDecisionReportingSecureService"
           binding="tns:ProsecutionDecisionReportingServiceSecureServiceSOAPBinding">
           <soap:address location="http://localhost/secure" />
       </wsdl:port>        
   </wsdl:service>
</wsdl:definitions>