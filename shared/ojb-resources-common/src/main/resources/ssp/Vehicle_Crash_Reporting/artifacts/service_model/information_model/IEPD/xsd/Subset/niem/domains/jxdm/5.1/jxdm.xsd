      <xs:extension base="nc:OrganizationType">
        <xs:sequence>
          <xs:element ref="j:MotorCarrierCategory" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="j:CommercialVehicle" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
