                    <lexs:DataItemID>12345678</lexs:DataItemID>
                    <lexs:DataItemContact>
                        <nc:PersonGivenName>Mary</nc:PersonGivenName>
                        <nc:PersonSurName>Smith</nc:PersonSurName>
                        <nc:ContactTelephoneNumber>
