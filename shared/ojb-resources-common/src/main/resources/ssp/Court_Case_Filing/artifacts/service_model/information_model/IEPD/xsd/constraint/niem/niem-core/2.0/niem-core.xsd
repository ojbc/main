    <xsd:annotation>
      <xsd:documentation>A unit of money or exchange.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="currencyText" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation>A unit of money or exchange.</xsd:documentation>
    </xsd:annotation>
  </xsd:attribute>
  <xsd:attribute name="languageCode" type="iso_639-3:LanguageCodeSimpleType">
