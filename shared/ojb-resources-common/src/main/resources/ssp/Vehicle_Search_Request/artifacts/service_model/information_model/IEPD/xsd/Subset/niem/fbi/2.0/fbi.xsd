      </xsd:enumeration>
      <xsd:enumeration value="LBL">
        <xsd:annotation>
          <xsd:documentation>BLUE, LIGHT</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="LGR">
        <xsd:annotation>
          <xsd:documentation>GREEN, LIGHT</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="MAR">
