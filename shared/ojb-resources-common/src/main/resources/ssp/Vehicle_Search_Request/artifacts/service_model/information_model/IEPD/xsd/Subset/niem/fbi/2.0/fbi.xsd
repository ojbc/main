      </xsd:enumeration>
      <xsd:enumeration value="BLU">
        <xsd:annotation>
          <xsd:documentation>BLUE</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BRO">
        <xsd:annotation>
          <xsd:documentation>BROWN</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="BRZ">
        <xsd:annotation>
          <xsd:documentation>BRONZE</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CAM">
        <xsd:annotation>
