      </xsd:enumeration>
      <xsd:enumeration value="COM">
        <xsd:annotation>
          <xsd:documentation>CHROME, STAINLESS STEEL</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CPR">
        <xsd:annotation>
          <xsd:documentation>COPPER</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CRM">
        <xsd:annotation>
          <xsd:documentation>CREAM, IVORY</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DBL">
        <xsd:annotation>
          <xsd:documentation>BLUE, DARK</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DGR">
        <xsd:annotation>
          <xsd:documentation>GREEN, DARK</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="GLD">
        <xsd:annotation>
          <xsd:documentation>GOLD</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="GRN">
        <xsd:annotation>
          <xsd:documentation>GREEN</xsd:documentation>
