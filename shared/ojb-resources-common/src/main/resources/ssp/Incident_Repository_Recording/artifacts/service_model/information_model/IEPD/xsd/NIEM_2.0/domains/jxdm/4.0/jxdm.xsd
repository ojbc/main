          <xsd:element ref="j:PassagePointSecurityCategoryText" minOccurs="0" maxOccurs="1"/>
          <xsd:element ref="j:PassagePointText" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="PersonAugmentationType">
