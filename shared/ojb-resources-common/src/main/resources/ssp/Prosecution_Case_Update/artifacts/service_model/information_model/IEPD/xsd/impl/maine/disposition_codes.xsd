	<xsd:element name="ChargeOriginCategoryCode" type="me_disp_codes:ChargeOriginCategoryCodeType" substitutionGroup="disp-ext:ChargeOriginCategory">
		<xsd:annotation>
			<xsd:documentation>A code that describes whether the charge is an original or amended charge.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SentenceConditionLiteralCode" type="me_disp_codes:SentenceConditionLiteralCodeType" substitutionGroup="disp-ext:SentenceConditionLiteral">
		<xsd:annotation>
			<xsd:documentation>A code that describes a sentence condition.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CourtActionCode" type="me_disp_codes:CourtActionCodeType" substitutionGroup="nc:DispositionCategory">
		<xsd:annotation>
			<xsd:documentation>A code that describes an action or judgement by a court related to a charge.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ChargeClassCode" type="me_disp_codes:ChargeClassCodeType" substitutionGroup="j:ChargeCategory">
		<xsd:annotation>
			<xsd:documentation>A code that describes the class or level of a charge.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="EnhancementCode" type="me_disp_codes:EnhancementCodeType" substitutionGroup="j:ChargeCategory">
		<xsd:annotation>
			<xsd:documentation>A code that describes a charge enhancement.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SentenceServedCode" type="me_disp_codes:SentenceServedCodeType" substitutionGroup="disp-ext:SentenceTimeCategory">
		<xsd:annotation>
			<xsd:documentation>A code that describes a sentence.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SentenceOriginCode" type="me_disp_codes:SentenceOriginCodeType" substitutionGroup="disp-ext:SentenceOriginCategory">
		<xsd:annotation>
			<xsd:documentation>A code that describes whether a sentence is 'original', 'corrected' or 'amended.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SentenceTermCategoryCode" type="me_disp_codes:SentenceTermCategoryCodeType" substitutionGroup="disp-ext:SentenceTimeCategory">
		<xsd:annotation>
			<xsd:documentation>A code that describes the type of time to be served.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>
